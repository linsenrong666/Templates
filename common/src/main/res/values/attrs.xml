<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!---->
    <declare-styleable name="common_label_container">
        <attr name="common_label_text" format="string"/>
        <attr name="common_label_text_size" format="dimension"/>
        <attr name="common_label_text_color" format="color"/>
        <attr name="common_label_text_gravity" format="enum">
            <enum name="left" value="1"/>
            <enum name="top" value="2"/>
            <enum name="right" value="3"/>
            <enum name="bottom" value="4"/>
            <enum name="center" value="5"/>
            <enum name="center_horizontal" value="6"/>
            <enum name="center_vertical" value="7"/>
        </attr>
        <attr name="common_content_text" format="string"/>
        <attr name="common_content_text_size" format="dimension"/>
        <attr name="common_content_text_color" format="color"/>
        <attr name="common_content_text_bold" format="boolean"/>
        <attr name="common_content_edit_size" format="dimension"/>
        <attr name="common_content_image" format="reference"/>
        <attr name="common_show_edit_text" format="boolean"/>
        <attr name="common_hint_text" format="string"/>
        <attr name="common_edit_input_type" format="integer"/>
        <attr name="common_content_height" format="dimension"/>
        <attr name="common_show_top_divider" format="boolean"/>
        <attr name="common_show_bottom_divider" format="boolean"/>
        <attr name="common_show_bottom_inner_divider" format="boolean"/>
        <attr name="common_inner_margin_left" format="dimension"/>
        <attr name="common_inner_margin_right" format="dimension"/>
        <attr name="common_inner_margin_top" format="dimension"/>
        <attr name="common_inner_margin_bottom" format="dimension"/>
        <attr name="common_inner_margin" format="dimension"/>
        <attr name="common_label_margin_right" format="dimension"/>
        <attr name="common_label_width" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="common_label_radioGroup">
        <attr name="common_label_text_rg" format="string"/>
        <attr name="common_label_text_size_rg" format="dimension"/>
        <attr name="common_content_height_rg" format="dimension"/>
        <attr name="common_show_top_divider_rg" format="boolean"/>
        <attr name="common_show_bottom_divider_rg" format="boolean"/>
        <attr name="common_show_bottom_inner_divider_rg" format="boolean"/>
        <attr name="common_inner_margin_left_rg" format="dimension"/>
        <attr name="common_inner_margin_right_rg" format="dimension"/>
        <attr name="common_label_width_rg" format="dimension"/>
        <attr name="common_content_list_rg" format="reference"/>
        <attr name="common_rb_drawable_id_rg" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="max_select" format="integer"/>
        <attr name="tag_gravity">
            <enum name="left" value="-1"/>
            <enum name="center" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="StarBar">
        <!--星星间距-->
        <attr name="starDistance" format="dimension"/>
        <!--星星大小-->
        <attr name="starSize" format="dimension"/>
        <!--星星个数-->
        <attr name="starCount" format="integer"/>
        <!--星星空图-->
        <attr name="starEmpty" format="reference"/>
        <!--星星满图-->
        <attr name="starFill" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WatchTextView">
        <attr name="watchLabelText" format="string"/>
        <attr name="watchShowLabel" format="boolean"/>
    </declare-styleable>

</resources>